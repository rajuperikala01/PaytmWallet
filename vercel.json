{
  "builds": [
    {
      "src": "apps/bank_webhook/api/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "node_modules/.prisma/**",
          "node_modules/@prisma/client/**",
          "apps/bank_webhook/node_modules/.prisma/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/bank_webhook/api/index.js"
    }
  ],
  "env": {
    "PRISMA_QUERY_ENGINE_LIBRARY": "./node_modules/.prisma/client/libquery_engine-rhel-openssl-3.0.x.so.node"
  }
}
